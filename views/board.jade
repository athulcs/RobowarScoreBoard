html
	head
		title Scoreboard
		link(rel="stylesheet" href="css/bootstrapmin.css")
		link(rel="stylesheet" href="css/googleapi.css")
		link(rel="stylesheet" href="css/board.css")
	body
		.text-center#scores
			.col-md-6#teamhome.score
			.col-md-6#teamaway.score
		#maintimercontainer.text-center.container
			h1#mainclock
		.text-center#bottom
			.col-md-6#shotclockcontainer
				h2#shotclocktimer
				h3#shotclock Shot Clock
			.col-md-6#tournament
				img.col-md-6#tournamentlogo.img.img-responsive
		audio#buzzer(src="sound/buzzer.mp3" preload="auto")
		script(src="socket.io/socket.io.js")
		script(src="js/jquerymin.js")
		script(src="js/bootsrap.js")
		script(src="js/board.js")